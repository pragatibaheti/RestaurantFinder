<% if(canceldata.length!=0) { 
for(var i=0;i<canceldata.length;i++){ %>
<h1><%= canceldata[i].hotelname %></h1>
<ul>
<li>Booking City : <%= canceldata[i].city %></li>
<li>Hotel Name : <%= canceldata[i].hotelname %></li>
<li>Booking Name : <%= canceldata[i].personname %></li>
<li>Booking Date : <%= canceldata[i].date %></li>
<li>Booking Time : <%= canceldata[i].time %></li>
</ul>
<br><br>
<% } } else { %>
<h2>Login</h2>
<h3>No cancel bookings available . </h3>
<form action='/cancelbooking' method='post'>
Enter your code here to confirm :
<br>
<input type="text" name="loggedcname">
<br><br>
<input type='submit' value="Confirm">
<br>
</form>
<% } %>
